/* ------------------------------------------
  NAVIGATION STYLES
--------------------------------------------- */

.fixed 
  position  fixed
  /*width  100%*/
  left  0
  top  0


.nav-collapse,
.nav-collapse * 
  -webkit-box-sizing  border-box
  -moz-box-sizing  border-box
  box-sizing  border-box


.nav-collapse,
.nav-collapse ul 
  list-style  none
  width  100%
  float  left
  margin-top 5.5px


@media screen and (min-width : 40em) 
  .nav-collapse 
    float right
    width auto
  
  #mobi-search
    display none
    
  #search-text
      display none
      
    #mobi-submit
      display none
    
  


.nav-collapse li 
  float left
  width 100%


@media screen and (min-width: 40em) 
  .nav-collapse li 
    width auto
  


.nav-collapse a 
  -webkit-tap-highlight-color rgba(0,0,0,0)
  border-top 1px solid white
  text-decoration none
  background #5682A3
  padding 0.7em 1em
  color #fff
  width 100%
  float left


.nav-collapse a:hover,
//.nav-collapse .active a 
  background #5682A3
  opacity 0.8


@media screen and (min-width: 40em) 
  .nav-collapse a 
    //border-left 1px solid white
    padding 1.02em 18px
    text-align center
    border-top 0
    float left
    font-size 18px
    margin-top 2px
    
  header  
    height 90px
  


.nav-collapse ul ul a 
  background #ca3716
  padding-left 2em


@media screen and (min-width: 40em) 
  .nav-collapse ul ul a 
    display none
  

header .search-bar 
  position relative
  overflow hidden
  margin-left 15px;
  margin-right 5px
  margin-top 12px
  float left
  

.nav-toggle 
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  text-decoration none
  text-indent -300px
  position relative
  overflow hidden
  margin-right 10px
  width 30px
  height 40px
  margin-top 13px
  //background #ff00ff
  float right


.nav-toggle:before 
  color #fff /* Edit this to change the icon color */
  font normal 38px "responsivenav" /* Edit font-size (28px) to change the icon size */
  text-transform none
  text-align center
  position absolute
  content "\2261" /* Hamburger icon */
  text-indent 0
  speak none
  width 100%
  left 0
  top 0


.nav-toggle.active:before 
  margin-top 10px
  font-size 24px
  content "\2261" /* Close icon */



